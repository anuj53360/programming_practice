package leetcode;

public class TwoSum {

	
public  static int[] twoSum(int[] nums, int target) {

	int []array=new int[2];
	int i=0;
	int sum=0;

	for (int j = array.length-1; j >=i; j--) {
     sum=array[i]+array[j];	
     if(sum<target) {
    	 i++;
     }
	}
	
	for (int k = 0; k < array.length; k++) {
		System.out.println(array[i]);
	} 
	
	return array;
    }
	
	public static void main(String[] args) {
		int[] nums= {2,7,11,15};
		int target=9;
		twoSum(nums,target);
	}
}
