package programmingpractice_programs;

public class Solution {

	 public static int  solution(String s) {
		int n=s.length();
		StringBuffer sb=new StringBuffer();
		String s1="";
		 for (int i = 0; i < n; i++)  
	           for (int j = i+1; j <= n; j++) 
	           
	        	   
	        	   s1=s.substring(i, j);
		 
		 return 0;
	        
	    }
	
	public static void main(String[] args) {
		String s="azABaabza";
		solution(s);
	}
}
