package binarytree;

public class ConnectNodeSameLevel {

	class Node {
		int val;
		Node left;
		Node right;
		Node nextright;

		Node(int val) {
			this.val = val;
			left = null;
			right = null;
			nextright = null;
		}
	}
	Node root;
	
	

	public static void main(String[] args) {
		ConnectNodeSameLevel connectnode = new ConnectNodeSameLevel();

	}
}
