package programmingpractice_programs;

public class Solution {

	 public static int  solution(String s) {
		String s1="";
		int n=s.length();
		 for (int i = 0; i < n; i++) { 
	           for (int j = i+1; j <= n; j++) { 
	            s1=s.substring(i, j);
		
		
		if(s1.length()>1) {
		for (int j2 = 0; j2 < s1.length(); j2++) {
			char c =Character.toUpperCase(s1.charAt(i)); 
				
			
			
		}	
		
		}
	           }
		 }
		 
		 
		 
		 return 0;
	        
	    }
	
	public static void main(String[] args) {
		String s="azABaabza";
		solution(s);
	}
}
