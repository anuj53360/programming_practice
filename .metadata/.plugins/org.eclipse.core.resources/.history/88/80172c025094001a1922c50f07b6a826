package leetcode;

public class MaxDepthHeightBinaryTree {

	class TreeNode{
		int val;
		TreeNode left;
		TreeNode right;
		public TreeNode(int val) {
			this.val=val;
			left=null;
			right=null;
		}
	}
	
	TreeNode root;
	
public int maxDepth(TreeNode root) {

	int lheight=maxDepth(root.left);
	int rheight=maxDepth(root.right);
	if(lheight>rheight) {
		return lheight+1;
	}else {
		return rheight+1;
	}
	
	return 0;
    }
	
	
	public static void main(String[] args) {
		
		
	}
}
