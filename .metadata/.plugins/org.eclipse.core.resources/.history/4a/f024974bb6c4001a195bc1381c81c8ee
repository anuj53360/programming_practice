			right = null;
		}
	}

	Node root;

	Node bstFromPreorder(int[] array, int n) {

		Index index = new Index();

		return root;
	}

	public static void main(String[] args) {

		int[] array = { 10, 5, 1, 7, 40, 50 };
		int n = array.length;
		ConstructBSTFromPreorder bstfpreorder = new ConstructBSTFromPreorder();
