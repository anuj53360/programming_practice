package programmingpractice_programs;

public class Solution {

	 public static int  solution(String s) {
		
		StringBuffer sb=new StringBuffer(s);
		int n=sb.length();
		 for (int i = 0; i < n; i++) { 
	           for (int j = i+1; j <= n; j++) { 
	        	   sb.substring(i, j);
		 
	           }
		 }
		 
		 
		 
		 return 0;
	        
	    }
	
	public static void main(String[] args) {
		String s="azABaabza";
		solution(s);
	}
}
