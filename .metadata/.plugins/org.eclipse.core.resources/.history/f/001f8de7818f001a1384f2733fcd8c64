package leetcode;

public class TwoSum {

	
public  static int[] twoSum(int[] nums, int target) {

	int []array=new int[2];
	int i=0;
	int j=nums.length;
	int sum=0;
	  while(i<j) {
		  sum=nums[i]+nums[j];
	  if(sum>target) {
		  j--;
	  }else {
		  i++;
	  }
	  }
	
	
	
	return nums;
    }
	
	public static void main(String[] args) {
		int[] nums= {2,7,11,15};
		int target=9;
		twoSum(nums,target);
	}
}
